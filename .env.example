# Applio Docker Environment Configuration
# Copy this file to .env and modify the values as needed

# Network Configuration
APPLIO_HOST=0.0.0.0
APPLIO_PORT=6969

# Application Settings
APPLIO_SHARE=false
APPLIO_DEBUG=false

# GPU Configuration (for GPU profile)
# Set to specific GPU IDs if you have multiple GPUs, leave empty for CPU-only
CUDA_VISIBLE_DEVICES=

# Optional: Uncomment and modify these if needed
# GRADIO_TEMP_DIR=/tmp/gradio
# GRADIO_FLAGGING_MODE=auto